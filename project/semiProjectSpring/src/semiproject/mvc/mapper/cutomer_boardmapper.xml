<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="board">
<insert id="add" parameterType="cbvo"> <!-- 게시글 작성 -->
insert into customer_board(c_num,c_writer,c_id,c_pwd,c_type,c_subject,c_content)
values(customer_board_seq.nextVal,#{c_writer},#{c_id},#{c_pwd},#{c_type},#{c_subject},#{c_content})
</insert>

